<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BluesMusicActivity"
            android:label="@string/blues_music"/>
        <activity android:name=".CountryMusicActivity"
            android:label="@string/country_music"/>
        <activity android:name=".JazMusicActivity"
            android:label="@string/jaz_music"/>
        <activity android:name=".PopMusicActivity"
            android:label="@string/pop_music"/>
        <activity android:name=".RapMusicActivity"
            android:label="@string/rap_music"/>
        <activity android:name=".RockMusicActivity"
            android:label="@string/rock_music"/>
        <activity android:name=".SearchBarActivity"
            android:label="Search">
        </activity>
    </application>

</manifest>